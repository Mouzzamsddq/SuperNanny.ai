<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_graph"
  app:startDestination="@id/nav_auth_fragment">

  <fragment
    android:id="@+id/nav_auth_fragment"
    android:name="com.example.suppernanny.presentation.auth.AuthFragment"
    tools:layout="@layout/fragment_auth">

    <action
      android:id="@+id/action_nav_auth_fragment_to_nav_home_fragment"
      app:destination="@id/nav_home_fragment" />

  </fragment>


  <fragment
    android:id="@+id/nav_home_fragment"
    android:name="com.example.suppernanny.presentation.home.HomeFragment"
    tools:layout="@layout/fragment_home" />

</navigation>